    <keyboard-shortcut first-keystroke="shift ctrl n" />
  </action>
  <action id="IncreaseColumnWidth" />
  <action id="IncrementWindowWidth" />
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="o" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="down" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="up" />
